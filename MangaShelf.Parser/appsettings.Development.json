{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information"
    }
  },
  "BackgroundWorker": {
    "Enabled": false,
    "DelayBetweenRuns": 1, // hours
    "StartDelay": 10 // seconds"
  },
  "ParserService": {
    "DelayBetweenParse": 5, // seconds
    "IgnoreExistingVolumes": true
  },
  "JobManager": {
    "DelayBetweenRuns": 1, // hours
    "MaxParallelParsers": 5,
    "ResetNextRun": true
  }
}
