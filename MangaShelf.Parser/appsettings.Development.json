{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "BackgroundWorker": {
    "Enabled": false,
    "DelayBetweenRuns": 1,
    "StartDelay": 10
  },
  "ParserService": {
    "DelayBetweenParse": 5,
    "IgnoreExistingVolumes": false
  },
  "JobManager": {
    "DelayBetweenRuns": 1,
    "MaxParallelParsers": 5,
    "ResetNextRun": true
  }
}
